{
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "rules": {
        "member-access": false,
        "max-classes-per-file": [true, 2],
        "no-empty-interface": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-console": false,
        "member-ordering": false,
        "jsx-no-lambda": false,
        "interface-name": false,
        "no-any": true
    },
    "defaultSeverity": "warning"
}
